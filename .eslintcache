Detail;\n","/Users/macbook/Desktop/programing/something/trouble-form/src/common/messages/ui/FormResults.ts",[],"/Users/macbook/Desktop/programing/something/trouble-form/src/reducks/commons/operatirons.ts",[],["181","182","183","184","185"],"/Users/macbook/Desktop/programing/something/trouble-form/src/reducks/commons/errorHandle.ts",["186","187"],"export const errorHandle = (e: any, funcName: string) => {\n  console.log(`error: ${funcName}`);\n  console.log(e);\n};\n","/Users/macbook/Desktop/programing/something/trouble-form/src/hooks/useForms.ts",[],{"ruleId":"188","replacedBy":"189"},{"ruleId":"190","replacedBy":"191"},{"ruleId":"192","replacedBy":"193"},{"ruleId":"194","replacedBy":"195"},{"ruleId":"196","replacedBy":"197"},{"ruleId":"188","replacedBy":"198"},{"ruleId":"190","replacedBy":"199"},{"ruleId":"192","replacedBy":"200"},{"ruleId":"194","replacedBy":"201"},{"ruleId":"196","replacedBy":"202"},{"ruleId":"203","severity":1,"message":"204","line":52,"column":7,"nodeType":"205","messageId":"206","endLine":52,"endColumn":21},{"ruleId":"203","severity":1,"message":"204","line":66,"column":7,"nodeType":"205","messageId":"206","endLine":66,"endColumn":24},{"ruleId":"203","severity":1,"message":"204","line":88,"column":5,"nodeType":"205","messageId":"206","endLine":88,"endColumn":24},{"ruleId":"207","severity":1,"message":"208","line":60,"column":3,"nodeType":"209","messageId":"206","endLine":60,"endColumn":14},{"ruleId":"207","severity":1,"message":"208","line":12,"column":3,"nodeType":"209","messageId":"206","endLine":12,"endColumn":14},{"ruleId":"207","severity":1,"message":"208","line":42,"column":5,"nodeType":"209","messageId":"206","endLine":42,"endColumn":16},{"ruleId":"203","severity":1,"message":"204","line":28,"column":7,"nodeType":"205","messageId":"206","endLine":28,"endColumn":31},{"ruleId":"203","severity":1,"message":"204","line":72,"column":7,"nodeType":"205","messageId":"206","endLine":72,"endColumn":21},{"ruleId":"207","severity":1,"message":"208"